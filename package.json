    "@types/chai": "^4.3.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/discord-rpc": "4.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/ip": "^1.1.0",
    "@types/js-yaml": "^4.0.5",
    "@types/langs": "^2.0.1",
    "@types/lodash": "^4.14.186",
    "@types/mocha": "^10.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.8.3",
    "@types/pg": "^8.6.5",
    "@types/pg-copy-streams": "^1.2.1",
    "@types/randomstring": "^1.1.8",
    "@types/readline-sync": "^1.4.4",
    "@types/semver": "^7.3.12",
    "@types/slug": "^5.0.3",
    "@types/tmi.js": "^1.8.1",
    "@types/uuid": "^8.3.4",
    "@types/which": "^2.0.1",
    "@types/yesql": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "5",
    "@typescript-eslint/parser": "5",
    "chai": "^4.3.6",
    "electron": "^21.1.0",
    "electron-notarize": "^1.2.1",
    "esbuild": "^0.15.10",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.1",
    "i18next-scanner": "^4.0.0",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "postgrator-cli": "^6.0.0",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "build": {
    "appId": "moe.karaokes.mugen.app",
    "afterSign": "build/notarize.cjs",
    "fileAssociations": [
      {
        "ext": "kmplaylist",
        "name": "Karaoke Mugen Playlist",
        "description": "Karaoke Mugen Playlist",
        "role": "Editor"
      },
      {
        "ext": "kmfavorites",
        "name": "Karaoke Mugen Favorites",
        "description": "Karaoke Mugen Favorites",
        "role": "Editor"
      }
    ],
    "nsis": {
      "warningsAsErrors": false,
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "license": "LICENSE.md",
      "differentialPackage": false
    },
    "deb": {
      "depends": [
        "gconf2",
        "gconf-service",
        "libnotify4",
        "libappindicator1",
        "libxtst6",
        "libnss3"
      ],
      "packageCategory": "video"
    },
    "dmg": {
      "icon": "build/DMG.icns",
      "contents": [
        {
          "x": 800,
          "y": 208,
          "name": "Karaoke Mugen.app"
        },
        {
          "x": 1000,
          "y": 208,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "compression": "normal",
      "publisherName": "Karaoke Mugen Team",
      "verifyUpdateCodeSignature": false,
      "publish": {
        "provider": "generic",
        "url": "https://mugen.karaokes.moe/downloads/"
      }
    },
    "mac": {
      "artifactName": "${productName}-${version}-mac-${arch}.${ext}",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "publish": {
        "provider": "generic",
        "url": "https://mugen.karaokes.moe/downloads/"
      },
      "hardenedRuntime": true,
      "entitlements": "build/entitlements.plist",
      "entitlementsInherit": "build/entitlements.plist",
      "gatekeeperAssess": false,
      "compression": "normal",
      "minimumSystemVersion": "10.14.0",
      "category": "public.app-category.entertainment"
    },
    "linux": {
      "target": [
        "deb",
        "tar.gz",
        "appImage",
        "flatpak"
      ],
      "vendor": "Karaoke Mugen Team",
      "executableName": "karaokemugen",
      "category": "AudioVideo",
      "synopsis": "Karaoke manager and player",
      "icon": "build/icon512.png",
      "publish": {
        "provider": "generic",
        "url": "https://mugen.karaokes.moe/downloads/"
      }
    },
    "flatpak": {
      "runtimeVersion": "21.08",
      "baseVersion": "21.08",
      "useWaylandFlags": true
    },
    "productName": "Karaoke Mugen",
    "copyright": "© 2017-2022 Karaoke Mugen Team",
    "extraResources": [
      "migrations/**",
      "locales/*",
      "assets/**",
      "kmfrontend/build/**",
      "initpage/**",
      "build/icon.png",
      "build/electronAboutWindow.css"
    ],
    "extraFiles": [
      "app/bin/**",
      {
        "from": "app-mac/bin/${arch}",
        "to": "app/bin"
      },
      {
        "from": "app-mac/bin/universal",
        "to": "app/bin"
      }
    ],
    "directories": {
      "output": "packages",
      "buildResources": "build"
    },
    "files": [
      "dist/*",
      "package.json",
      "!**/node_modules/**/*"
    ],
    "protocols": [
      {
        "name": "km",
        "role": "Viewer",
        "schemes": [
          "km"
        ]
      }
    ]
  },
  "lint-staged": {
    "!(src/lib|assets/guestAvatars)": "prettier --write --ignore-unknown"
  },
  "packageManager": "yarn@1.22.1"
}
